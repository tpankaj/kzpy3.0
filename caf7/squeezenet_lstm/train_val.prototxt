}

layer {
	name: "ip1"
	type: "InnerProduct"
	bottom: "pool10"
	top: "ip1"
	inner_product_param {
		num_output: 2184
		weight_filler {
			type: "xavier" 
		}
	}
}	

layer {
	name: "ip1_relu"
	type: "ReLU"
	bottom: "ip1"
	top: "ip1"
}

