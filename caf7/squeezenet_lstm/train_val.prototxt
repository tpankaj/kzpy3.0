	name: "ip2"
	type: "InnerProduct"
	bottom: "ip1"
	top: "ip2"
	inner_product_param {
		num_output: 1024
		weight_filler {
			type: "xavier" 
		}
	}
}

layer {
   name: "ip2_reshape"
   type: "Reshape"
   bottom: "ip2"
   top: "ip2_reshape"
   reshape_param {
      shape {
         dim: 0
         dim: 1
         dim: -1
      }
   }
}

#####################   LSTM layer ######################
layer {
      name: "lstm1"
      type: "LSTM"
      bottom: "ip2_reshape"
      bottom: "clip"
      top: "lstm1"
      recurrent_param {
        num_output: 256
	weight_filler {
	  type: "gaussian"
	  std: 0.1
	}
        bias_filler {
          type: "constant"
        }
      }
}

############# Final fully connected layer ###############
layer {
  name: "ip3"
  type: "InnerProduct"
  bottom: "lstm1"
  top: "ip3"
  param {
    lr_mult: 10
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 2
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
    axis: 2
  }
}

layer {
	name: "euclidean"
	type: "EuclideanLoss"
	bottom: "steer_motor_target_data"
	bottom: "ip3"
	top: "euclidean"
	loss_weight: 1
}
