
layer {
	name: "steer_motor_target_data"
	type: "DummyData"
	top: "steer_motor_target_data"
	dummy_data_param {
		shape {
            dim: 10 # time samples
			dim: 1 # number of independent channels
			dim: 2 # steer/motor
		}
	}
}

layer {
	name: "metadata"
	type: "DummyData"
	top: "metadata"
	dummy_data_param {
		shape {
			dim: 10
			dim: 6
			dim: 94
			dim: 168
		}
	}
}

layer {
    name: "clip"
    type: "DummyData"
    top: "clip"
    dummy_data_param {
      shape {
        dim: 10 # time samples
        dim: 1 # number of independent channels
      }
    }
}

layer {
	name: "ZED_data_pool2"
	type: "DummyData"
	top: "ZED_data_pool2"
	dummy_data_param {
		shape {
			dim: 10 # time samples
            dim: 1 # number of independent channels
			dim: 2 # depth channel
			dim: 3 # color channel
            dim: 94 # height channel
			dim: 168 # width channel
		}
	}
}


layer {
  name: "ZED_data_pool2_scale"
  type: "Scale"
  bottom: "ZED_data_pool2"
  top: "ZED_data_pool2_scale"
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  scale_param {
    filler {
      value: 0.003921    }
    bias_term: true
    bias_filler {
      value: -0.5
    }
  }
}

layer {
   name: "ZED_data_pool2_reshape"
   type: "Reshape"
   bottom: "ZED_data_pool2_scale"
   top: "ZED_data_pool2_reshape"
   reshape_param {
      shape {
         dim: 0 # copy number of time samples
         dim: 6 # total number of images per sample
         dim: 94 # height channel
         dim: 168 # width channel
      }
   }
}

layer {
      name: "ZED_metadata_concat"
      type: "Concat"
      bottom: "ZED_data_pool2_reshape"
      bottom: "metadata"
      top: "ZED_metadata_concat"
      concat_param {
          axis: 1
      }
}

layer {
  name: "conv1"
  type: "Convolution"
  bottom: "ZED_metadata_concat"
  top: "conv1"
  convolution_param {
    num_output: 64
    kernel_size: 3
    stride: 2
    weight_filler {
      type: "xavier"
    }
  }
}
layer {
  name: "relu_conv1"
  type: "ReLU"
  bottom: "conv1"
  top: "conv1"
}
layer {
  name: "pool1"
  type: "Pooling"
  bottom: "conv1"
  top: "pool1"
